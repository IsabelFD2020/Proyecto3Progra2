<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Crud</servlet-name>
        <servlet-class>clases.Crud</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Entrar</servlet-name>
        <servlet-class>clases.Entrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consultar</servlet-name>
        <servlet-class>Servelets.Consultar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioOrden</servlet-name>
        <servlet-class>Servelets.ServicioOrden</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servicio</servlet-name>
        <servlet-class>Servelets.Servicio</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Crud</servlet-name>
        <url-pattern>/Crud</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Entrar</servlet-name>
        <url-pattern>/Entrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consultar</servlet-name>
        <url-pattern>/Consultar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioOrden</servlet-name>
        <url-pattern>/ServicioOrden</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servicio</servlet-name>
        <url-pattern>/Servicio</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
